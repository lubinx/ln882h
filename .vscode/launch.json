      "setupCommands": [
        {
          "text": "-enable-pretty-printing",
        }
      ],
      "postRemoteConnectCommands": [
        {
          "text": "set mem inaccessible-by-default off"
        },
        {
          "text": "monitor reset"
        },
        {
          "text": "load"
        },
        {
          "text": "monitor reset"
        },
      ]
